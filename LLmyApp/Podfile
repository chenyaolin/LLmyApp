source 'https://mirror.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'

platform :ios, '9.0'
use_frameworks!
inhibit_all_warnings!

def shared_pods
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'Moya/RxSwift'
    pod 'SwiftLint'
    pod 'HandyJSON', '~> 5.0.0-beta'
    pod 'Kingfisher'
end

target 'LLmyApp' do
	shared_pods
end
